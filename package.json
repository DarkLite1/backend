{
  "name": "graphql-backend",
  "version": "0.0.1",
  "devDependencies": {
    "@types/express": "^4.17.6",
    "@types/jest": "^25.2.3",
    "@types/node": "^12.12.44",
    "apollo-boost": "^0.4.9",
    "env-cmd": "^10.1.0",
    "graphql-request": "^2.0.0",
    "jest": "^26.0.1",
    "ts-jest": "^26.1.0",
    "ts-node-dev": "^1.0.0-pre.44",
    "typescript": "^3.9.5"
  },
  "dependencies": {
    "apollo-server-express": "^2.14.2",
    "express": "^4.17.1",
    "graphql": "^14.5.8",
    "mssql": "^6.2.0",
    "reflect-metadata": "^0.1.10",
    "ts-node": "^8.10.2",
    "type-graphql": "^0.17.5",
    "typeorm": "^0.2.25"
  },
  "scripts": {
    "start": "ts-node --transpile-only src/index.ts",
    "test": "env-cmd -e test jest --runInBand",
    "dev": "env-cmd -e dev ts-node-dev --type-check --respawn src/index.ts",
    "typeorm-migration-generate": "./node_modules/.bin/ts-node ./node_modules/typeorm/cli.js migration:generate -n MigrationName -c it-portal",
    "typeorm-migration-run": "./node_modules/.bin/ts-node ./node_modules/typeorm/cli.js migration:run -c it-portal",
    "typeorm-migration-revert": "./node_modules/.bin/ts-node ./node_modules/typeorm/cli.js migration:revert -c it-portal",
    "typeorm-migration-show": "./node_modules/.bin/ts-node ./node_modules/typeorm/cli.js migration:show"
  }
}
